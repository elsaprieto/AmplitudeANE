<extension xmlns="http://ns.adobe.com/air/extension/15.0">
    <id>com.pilipoplabs.AmplitudeANE</id>
        <versionNumber>1.0</versionNumber>
        <platforms>
        <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libAmplitudeANE.a</nativeLibrary>
                <initializer>AmplitudeANEInitializer</initializer>
                <finalizer>AmplitudeANEFinalizer</finalizer>
            </applicationDeployment>
        </platform>

        <platform name="Android-ARM"> 
            <applicationDeployment> 
                <nativeLibrary>libAmplitudeANE.jar</nativeLibrary> 
                <initializer>com.pilipoplabs.amplitudeane.Extension</initializer> 
                <finalizer>com.pilipoplabs.amplitudeane.Extension</finalizer>
            </applicationDeployment> 
        </platform>

        <platform name="default"> 
            <applicationDeployment/> 
        </platform> 
    </platforms>
</extension>